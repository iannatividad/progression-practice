{
    "name": "progression",
    "version": "0.1.0",
    "description": "Repo for practicing techs in progression.",
    "private": true,
    "dependencies": {
        "@cucumber/cucumber": "^7.0.0",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.22",
        "@types/node": "^12.20.7",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "cucumber-console-formatter": "^1.0.0",
        "expect": "^26.6.2",
        "polished": "^4.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.13.0",
        "react-query-devtools": "^2.6.3",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "styled-bootstrap-grid": "^3.1.0",
        "styled-components": "^5.2.1",
        "typescript": "^4.2.3",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "cucumber": "node ./node_modules/@cucumber/cucumber/bin/cucumber-js tests/features/**/*.feature --require tests/step-definitions/**.ts --require-module ts-node/register --format cucumber-console-formatter --publish-quiet",
        "test": "npm run cucumber",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "react:test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/expect": "^24.3.0",
        "@types/react-query": "^1.1.2",
        "@types/react-router-dom": "^5.1.7",
        "@types/styled-components": "^5.1.9",
        "rollup-plugin-node-polyfills": "^0.2.1"
    }
}
